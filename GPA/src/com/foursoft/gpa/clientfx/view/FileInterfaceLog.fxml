<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.foursoft.gpa.clientfx.controller.FileInterfaceLogController">
   <center>
      <TableView fx:id="errorGrid" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
         <columns>
            <TableColumn fx:id="fileNameColumn" prefWidth="230.0" text="File Name" />
            <TableColumn fx:id="dateColumn" minWidth="0.0" prefWidth="99.0" text="Date" />
            <TableColumn fx:id="timeColumn" prefWidth="116.0" text="Time" />
            <TableColumn fx:id="errorColumn" prefWidth="498.0" text="Error" />
         </columns>
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
      </TableView>
   </center>
   <bottom>
      <HBox prefHeight="55.0" prefWidth="945.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="selectFileButton" mnemonicParsing="false" prefHeight="27.0" prefWidth="69.0" text="Select File" BorderPane.alignment="CENTER">
               <HBox.margin>
                  <Insets top="10.0" />
               </HBox.margin>
            </Button>
            <VBox fx:id="progressContainer" prefHeight="109.0" prefWidth="589.0">
               <padding>
                  <Insets top="10.0" />
               </padding>
            </VBox>
            <Button fx:id="buttonSync" mnemonicParsing="false" text="Stop" visible="false" />
         </children>
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
      </HBox>
   </bottom>
</BorderPane>
